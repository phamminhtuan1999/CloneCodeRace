
<!DOCTYPE html>

<html lang="en">
	<!--begin::Head-->
	<head>
		<title>DP2T</title>
        <link href="/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
        <% include partials/head %>

		<script type="module">
			import { LaTeXJSComponent } from "../latex.js-0.12.4/package/dist/latex.mjs"
			customElements.define("latex-js", LaTeXJSComponent)
		  </script>
	</head>
	<!--end::Head-->
	<!--begin::Body-->
	<body id="kt_body" style="background-image: url(/media/bg/bg-10.jpg)" class="quick-panel-right demo-panel-right offcanvas-right header-fixed subheader-enabled page-loading">
		<!--begin::Main-->

		<div class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div class="d-flex flex-row flex-column-fluid page">
				<!--begin::Wrapper-->
				<div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
		        <% include partials/header %>
                		<!--begin::Content-->
					<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
						<!--begin::Subheader-->
						<div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
							<div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
								<!--begin::Info-->
								<div class="d-flex align-items-center flex-wrap mr-1">
									<!--begin::Heading-->
									<div class="d-flex flex-column">
										<!--begin::Title-->
										<!--<h2 class="text-white font-weight-bold my-2 mr-5">Luyện tập</h2>-->
										<!--end::Title-->
										<!--begin::Breadcrumb-->
										<div class="d-flex align-items-center font-weight-bold my-2">
											<!--begin::Item-->
											<a href="#" class="opacity-75 hover-opacity-100">
												<i class="flaticon2-shelter text-white icon-1x"></i>
											</a>
											<!--end::Item-->
											<!--begin::Item-->
											<span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
											<a href="" class="text-white text-hover-white opacity-75 hover-opacity-100">Trang Chủ</a>
											<!--end::Item-->
											<!--begin::Item-->
											<span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
											<a href="" class="text-white text-hover-white opacity-75 hover-opacity-100">Luyện tập</a>
											<!--end::Item-->
										</div>
										<!--end::Breadcrumb-->
									</div>
									<!--end::Heading-->
								</div>
								<!--end::Info-->

							</div>
						</div>
						<!--end::Subheader-->
						<!--begin::Entry-->
						<div class="d-flex flex-column-fluid">
							<!--begin::Container-->
							<div class="container">
                                <!--begin::Dashboard-->
                                <div class="card card-custom">
                                    <!--begin::Row-->
                                <div class="row">
                                    <div class="card-body">
										
										
                                        <!--begin: Datatable-->
                                        <table id="table_id" class="table table-separate table-head-custom table-checkable" data-page-length="50" style="text-align:center">
                                            <thead style="text-align:center">
                                                <tr>
                                                    <th style="width:2%">STT</th>
                                                    <th style="width:3%">ID</th>
                                                    <th style="width:38%">Đề bài</th>
                                                    <th style="width:10%">Chủ đề</th>
                                                    <th style="width:10%">Độ khó</th>
                                                    <th style="width:10%">Độ khó khách quan</th>
													<th style="width:10%">Làm thành công</th>
                                                    </tr>
                                            </thead>
                                            <tbody style="text-align:center">
                                                <% for(var i = 0; i < problems.length; i++) { %>
                                                    <tr>
                                                        <td>
                                                            
                                                        </td>
                                                        <td style="width: 5%;height: 90px;"><a href="/problem/<%= problems[i].qID %>">
                                                                <%= problems[i].qID %></a></td>
                                                        <td style="width: 55%;height: 90px;"><a href="/problem/<%= problems[i].qID %>" style="text-decoration: none;">
                                                                <latex-js><%= problems[i].name %></latex-js> &ensp;</a></td>
														<td style="width: 20%;height: 90px;">	
																<%= problems[i].tags %>															
                                                        </td>
                                                        <td style="width: 5%;height: 90px;">
                                                                <% if (problems[i].difficulty == 0.1) { %>1<% } %>
                                                                <% if (problems[i].difficulty == 0.2) { %>2<% } %>
                                                                <% if (problems[i].difficulty == 0.3) { %>3<% } %>
                                                                <% if (problems[i].difficulty == 0.4) { %>4<% } %>
                                                                <% if (problems[i].difficulty == 0.5) { %>5<% } %>
                                                                <% if (problems[i].difficulty == 0.6) { %>6<% } %>
                                                                <% if (problems[i].difficulty == 0.7) { %>7<% } %>
                                                                <% if (problems[i].difficulty == 0.8) { %>8<% } %>
                                                                <% if (problems[i].difficulty == 0.9) { %>9<% } %>
                                                                <% if (problems[i].difficulty == 1.0) { %>10<% } %>
                                                            </td>
                                                        <td style="width: 5%;height: 90px;">
                                                                <% if (problems[i].difficultyAutoUpdate == 0.1) { %>1<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.2) { %>2<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.3) { %>3<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.4) { %>4<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.5) { %>5<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.6) { %>6<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.7) { %>7<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.8) { %>8<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 0.9) { %>9<% } %>
                                                                <% if (problems[i].difficultyAutoUpdate == 1.0) { %>10<% } %>
                                                        </td>    
                                                        <% var sol = 0; %>
														<% if(solved[problems[i].qID]) {sol = solved[problems[i].qID]; }%>
														<td style="width: 15%;height: 90px;">
															<%= sol %>
														</td> 
													
                                                    </tr>
                                                    <% } %>
                                        </table>
                                        <!--end: Datatable-->
                                    </div>
                                </div>

                                </div>
								
							</div>
							<!--end::Container-->
						</div>
						<!--end::Entry-->
					</div>
					<!--end::Content-->
				<% include partials/footer %>
			</div>
			<!--end::Wrapper-->
		</div>
			<!--end::Page-->
	</div>
    <!--end::Main-->
  
    <!--begin::Scrolltop-->
    <div id="kt_scrolltop" class="scrolltop">
    <span class="svg-icon">
    <!--begin::Svg Icon | path:assets/media/svg/icons/Navigation/Up-2.svg-->
    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
            <polygon points="0 0 24 0 24 24 0 24" />
