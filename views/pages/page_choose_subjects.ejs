
<!DOCTYPE html>

<html lang="en">
	<!--begin::Head-->
	<head>
    <title>DP2T</title>
	<% include partials/head %>

    <style>
        .contact-form-submit {
            color: white;
            padding: 0.5rem;
            margin-bottom:5px;
            width: 100%;
            max-width: 15rem;
            border-radius: 5rem;
            border: 0;
            outline: none;
            cursor: pointer;
            text-align: center;
            font-weight: 500;
            white-space: nowrap;
            letter-spacing: 1px;
            background-color: #7b78f8;
            box-shadow: 0 10px 20px 0 rgba(123, 120, 248, 0.5);
            box-sizing: border-box;
        }
    </style>

	</head>
	<!--end::Head-->
	<!--begin::Body-->
	<body id="kt_body" style="background-image: url(/media/bg/bg-10.jpg)" class="quick-panel-right demo-panel-right offcanvas-right header-fixed subheader-enabled page-loading">
		<!--begin::Main-->

		<div class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div class="d-flex flex-row flex-column-fluid page">
				<!--begin::Wrapper-->
				<div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
		        <% include partials/header %>
                <!--begin::Content-->
					<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
						<!--begin::Subheader-->
						<div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
							<div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
								<!--begin::Info-->
								<div class="d-flex align-items-center flex-wrap mr-1">
									<!--begin::Heading-->
									<div class="d-flex flex-column">
										<!--begin::Title-->
										<h2 class="text-white font-weight-bold my-2 mr-5">Môn học</h2>
										<!--end::Title-->
										<!--begin::Breadcrumb-->
										<div class="d-flex align-items-center font-weight-bold my-2">
											<!--begin::Item-->
											<a href="#" class="opacity-75 hover-opacity-100">
												<i class="flaticon2-shelter text-white icon-1x"></i>
											</a>
											<span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
											<a href="" class="text-white text-hover-white opacity-75 hover-opacity-100">Trang chủ</a>
											<!--end::Item-->
											<!--begin::Item-->
											<span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
											<a href="" class="text-white text-hover-white opacity-75 hover-opacity-100">Môn học</a>
											<!--end::Item-->
											<!--end::Item-->
										</div>
										<!--end::Breadcrumb-->
									</div>
									<!--end::Heading-->
								</div>
								<!--end::Info-->

							</div>
						</div>
						<!--end::Subheader-->
						<!--begin::Entry-->
						<div class="d-flex flex-column-fluid">
							<!--begin::Container-->
							<div class="container">
                                <!--begin::Dashboard-->
                                <div class="card card-custom">
                                    <!--begin::Row-->
                                	<div class="row">
                                    	<div class="card-body">
											<!--begin::Card-->
                                            <!--Document_1-->
                                            <% for(var i=0;i<data.length; i++){ %> 
											<div class="card card-custom gutter-b">
												<div class="card-body" style="padding: 0.75rem 2.0rem;">
													<!--begin::Top-->
													<div class="d-flex">
														<!--begin::Pic-->
														<div class="flex-shrink-0 mr-7">
															<div class="symbol symbol-50 symbol-lg-120">
																<img alt="Pic" src="/images.jpeg" />
															</div>
														</div>
														<!--end::Pic-->
														<!--begin: Info-->
														<div class="flex-grow-1" style="margin:auto;">
															<!--begin::Title-->
															<div class="d-flex align-items-center justify-content-between flex-wrap mt-2" style="margin-bottom: 20px;">
																<!--begin::User-->
																<div class="mr-3" style="text-align:center">
																	<!--begin::Name-->
																	<a href="/question_subjects/<%= data[i]._id %> " class="d-flex align-items-center text-dark text-hover-primary font-size-h5 font-weight-bold mr-3"><%= data[i].subjectsName %>
																	<i class="flaticon2-correct text-success icon-md ml-2"></i></a>
																	<!--end::Name-->
																	
																</div>
																<!--begin::User-->
																<!--begin::Actions-->
																<div class="my-lg-0 my-1">
																</div>
																<!--end::Actions-->
															</div>
															<!--end::Title-->
															<!--begin::Content-->
															
															<!--end::Content-->
														</div>
														<!--end::Info-->
													</div>
													<!--end::Top-->
													
												</div>
                      				        </div>
                                             <% } %> 
											<!--end::Card-->

                                    	</div>
                               		 </div>
                                </div>
								
							</div>
							<!--end::Container-->
						</div>
                        <!--end::Entry-->
					</div>
				<% include partials/footer %>
			</div>
			<!--end::Wrapper-->
		</div>
			<!--end::Page-->
	</div>
    <!--end::Main-->
  
